citHeader("To cite mixlow in publications use:")

citEntry(entry = "Article",
  title        = "An {R} Package for Assessing Drug Synergism/Antagonism",
  author       = personList(as.person("John C. Boik"),
                   as.person("Balasubramanian Narasimhan")),
  journal      = "Journal of Statistical Software",
  year         = "2010",
  volume       = "34",
  number       = "6",
  pages        = "1--18",
  url          = "http://www.jstatsoft.org/v34/i06/",

  textVersion  =
  paste("John C. Boik, Balasubramanian Narasimhan (2010).",
        "An R Package for Assessing Drug Synergism/Antagonism.",
        "Journal of Statistical Software, 34(6), 1-18.",
        "URL http://www.jstatsoft.org/v34/i06/.")
)

